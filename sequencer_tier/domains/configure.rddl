class DialogueSV : StateVariable {

    predicate Interact(string intent) {
        duration >= 10.0;
    }
}

predicate GatherProfile() : Interval {
    goal gp = new dialogue_sv.Interact(intent: "gather_profile", start: start, duration: duration, end: end);
}

predicate Configure() : Interval {
    goal gp = new GatherProfile();
    gp.start >= start; gp.end <= end;
}

DialogueSV dialogue_sv = new DialogueSV();
goal cnf = new Configure();